models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable', password: string, remember_token: 'string:100 nullable' }
    PaymentMethod: { method: 'string:400 unique' }
    Department: { name: 'string:400 unique', relationships: { hasMany: City } }
    City: { name: 'string:400 unique', relationships: { belongsTo: Department } }
    ReturnAlert: { status: 'enum:Top,Medium,Low unique' }
    Shop: { name: 'string:400 unique', relationships: { hasMany: Seller } }
    Seller: { name: 'string:400', last_name: 'string:400', relationships: { belongsTo: Shop } }
    Segmentation: { type: 'string:400 unique' }
